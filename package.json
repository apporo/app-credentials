{
  "name": "app-tokenify",
  "version": "0.2.2",
  "description": "Devebot token-based authentication layerware",
  "license": "MIT",
  "homepage": "https://github.com/apporo/app-tokenify.git",
  "author": {
    "name": "apporo",
    "email": "apporo@devebot.com",
    "url": "https://github.com/apporo"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "bin/",
    "config/",
    "data/",
    "lib/"
  ],
  "scripts": {
    "build": "babel ./src -d ./lib",
