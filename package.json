    "prepublish": "npm run clean && NODE_ENV=production npm run build",
    "pretest": "npm run clean && NODE_ENV=test npm run build",
    "test": "cucumber-js ./test/bdd/features/*.feature",
    "verify": "npm run prepublish && cucumber-js ./test/bdd/features/*.feature"
  },
  "keywords": [
    "devebot",
    "devebot-plugin"
  ],
  "peerDependencies": {
    "app-webweaver": "^0.2.2",
    "devebot": "^0.2.6"
  },
  "dependencies": {
    "app-webweaver": "^0.2.2",
    "basic-auth": "^1.0.4",
    "bcryptjs": "^2.3.0",
    "devebot": "^0.2.6",
    "jsonwebtoken": "^7.1.9",
    "node-cache": "^4.1.1",
    "request": "^2.75.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-minify-mangle-names": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "benchmark": "^2.1.4",
    "chai": "^3.5.0",
    "cucumber": "^1.3.1",
    "mock-http-server": "0.0.4"
  }
